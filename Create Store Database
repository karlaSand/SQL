CREATE TABLE store (id integer primary key, name text, color text, quantity integer, price integer);

Insert into store values (1, "skates", "blue", 6, "$49.99");
insert into store values (2, "skates", "purple", 8, "$79.99");
insert into store values (3, "skates", "pink", 5, "$59.99");
insert into store values (4, "skates", "black", 9, "$69.99");
insert into store values (5, "skates", "white", 10, "$69.99");
